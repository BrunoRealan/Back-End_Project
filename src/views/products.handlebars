<h1>Bienvenido {{first_name}} {{last_name}} a la tienda BackEnd</h1>
<h3>Estos son los siguientes productos:</h3>

{{#each response.payload}}
  <p>Nombre: {{title}}</p>
  <p>Descripción: {{description}}</p>
  <p>Código: {{code}}</p>
  <p>Precio: {{price}}</p>
  <p>Status: {{status}}</p>
  <p>Stock: {{stock}}</p>
  <p>Categoría: {{category}}</p>
  <button class="addToCartButton" data-product-id="{{_id}}">Agregar al Carrito</button>
  <br />
  <br />
  <br />
{{/each}}

<a href="{{response.prevLink}}">Pagina Anterior: {{response.prevPage}}</a>
<br />
<a href="{{response.nextLink}}">Pagina Siguiente: {{response.nextPage}}</a>
<br />
<br />
<br />
<button id="logoutButton">Desloguear!</button>

<script type="module" src="/js/products.js"></script>
<script type="module" src="/js/logout.js"></script>